{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": false
    },
    "paths": {
      "~/*": ["./src/*"],
      "@internal/*": ["./src/internal-package/*"],
      "@lib/*": ["./src/lib/*"]
    },
    "baseUrl": "./",
    "target": "es2022",
    "loose": false,
    "externalHelpers": false
  },
  "minify": false
}
